programa {

  real mat1, mat2,  mat3, mat4, mat5, mat6
  real tempo = 10.40
  real estudo = (tempo / 0.6) 
  real tplivre = 24.00 - tempo
  funcao inicio() {
    escreva("se fulano de tal usasse o mesmo tempo de estudo para todas as suas materias, ele gastaria ", estudo," minutos por disciplina, e lhe sobraria ", tplivre ," horas de tempo livre")

  }
}
